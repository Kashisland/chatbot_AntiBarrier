{
	"expo": {
		"name": "chatbot-app",
		"slug": "chatbot-app",
		"version": "1.0.0",
		"entryPoint": "./node_modules/expo/AppEntry.js",
		"splash": {
			"image": "./assets/logo.png",
			"resizeMode": "contain",
			"backgroundColor": "#00BCD4"
		},
		"plugins": [
			[
				"expo-av",
				{
					"microphonePermission": "Allow to access your microphone."
				}
			],
			"expo-secure-store"
		]
	}
}
